; 27 May 2022
; Dz_Eng_testing
; Vary delay between Pi/2 X and Y pulses

#include <Avance.incl>
define list<delay> dlist={0.000005 0.000010 0.000015 0.000020 0.000025}
1   ze

2   d1
    1m pl2:f2

3   (p1 ph1):f2
    dlist
    (p1 ph2):f2


    go=2 ph31

    1m wr #0 if #0

    5m dlist.inc
    lo to 2 times td1


    exit


ph1 = 0 2 2 0
ph2 = 1 3 3 1

ph31 = 1 3 3 1
