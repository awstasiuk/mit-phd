; 28 July 2022
; Andrew Stasiuk
;
; ZZ Correlation under periodic kicking
; Discrete Time Crystal Experiments
; Scrambling time variable
; Disorder Strenght Variable
; 9 deg Framechange
; theta=175

; tau >= 10us (delay primitive for echo sequence)

define list<delay> delta = {0.000000 0.000005 0.000010 0.000015 0.000020}

define delay tau
"tau = (d15-delta)/2"

define delay tau1
"tau1 = (tau-2*p1-.0000025)/2+delta/2"
define delay tau0
"tau0 = tau-2*p1-.0000025"

"l1=0"

1  ze

2   ; state prep
    d1 rpp2 rpp3 rpp4 rpp5 rpp6 rpp7
    100u pl2:f2

    (p1 ph0):f2
    d2
    (p2 ph1):f2
    d13

3   ; Echo
    tau1

    (p1 ph2^):f2
    2.5u
    (p1 ph3^):f2

    tau0

    (p1 ph4^):f2
    2.5u
    (p1 ph5^):f2

    tau1

4   ; Rx(theta)
    (p1 ph6^):f2
    2.5u
    (p1 ph7^):f2

    lo to 3 times l1

    ; measure
    d13
    (p2 ph8):f2

    go=2 ph31
    1m wr #0 if #0

    ; increase number of kicking cycles
    1m iu1
    lo to 2 times td2

    5m ru1
    5m delta.inc
    lo to 2 times td1

  exit

ph0 = (360) {351}*2 {171}*2
ph1 = {1}*2 {3}*2

ph2 = (360) 0 239 118 357 236 115 354 233 112 351 230 109 348 227 106 345 224 103 342 221 100 339 218 97 336 215 94 333 212 91 330 209 88 327 206 85 324 203 82 321 200 79 318 197 76 315 194 73 312 191
ph3 = (360) 9 248 127 6 245 124 3 242 121 0 239 118 357 236 115 354 233 112 351 230 109 348 227 106 345 224 103 342 221 100 339 218 97 336 215 94 333 212 91 330 209 88 327 206 85 324 203 82 321 200
ph4 = (360) 198 77 316 195 74 313 192 71 310 189 68 307 186 65 304 183 62 301 180 59 298 177 56 295 174 53 292 171 50 289 168 47 286 165 44 283 162 41 280 159 38 277 156 35 274 153 32 271 150 29
ph5 = (360) 207 86 325 204 83 322 201 80 319 198 77 316 195 74 313 192 71 310 189 68 307 186 65 304 183 62 301 180 59 298 177 56 295 174 53 292 171 50 289 168 47 286 165 44 283 162 41 280 159 38

ph6 = (360) 306 185 64 303 182 61 300 179 58 297 176 55 294 173 52 291 170 49 288 167 46 285 164 43 282 161 40 279 158 37 276 155 34 273 152 31 270 149 28 267 146 25 264 143 22 261 140 19 258 137
ph7 = (360) 320 199 78 317 196 75 314 193 72 311 190 69 308 187 66 305 184 63 302 181 60 299 178 57 296 175 54 293 172 51 290 169 48 287 166 45 284 163 42 281 160 39 278 157 36 275 154 33 272 151

ph8 = 1 3 3 1
ph31 = 0 2 2 0
