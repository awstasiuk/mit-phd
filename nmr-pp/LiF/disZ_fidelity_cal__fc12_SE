; Andrew Stasiuk


; WAHUHA8 delays
define delay delay0
"delay0=(2*d20-p1)/2"
define delay delay1
"delay1=d20-p1"
define delay delay2
"delay2=(2*d20-p1)"

"l1=0"

1  ze

    5m ip15*36
    5m ip16*36
    5m ip17*36
    5m ip18*36
    5m ip19*36

2   ; state & pp reset delay
    d1 rpp1 rpp2 rpp3 rpp4 rpp15 rpp16 rpp17 rpp18
    100u pl2:f2

; Random Zeeman State Preparation
    (p1 ph0):f2

3   ; Repeated WAHUHA8 to encode random state 
    delay0
    (p1 ph1^):f2
    delay1
    (p1 ph2^):f2
    delay2
    (p1 ph3^):f2
    delay1
    (p1 ph4^):f2
    delay0
    lo to 3 times l1

    (p1 ph5):f2
    

; Hamiltonian Evolution
4  2.5u ; something goes here eventually



; Random Zeeman Observable Engineering
    (p1 ph14):f2

5   ; Repeated WAHUHA8 to encode random observable (18 cycles)
    delay0
    (p1 ph15^):f2
    delay1
    (p1 ph16^):f2
    delay2
    (p1 ph17^):f2
    delay1
    (p1 ph18^):f2
    delay0
    lo to 5 times l1


    ; shelf in Z, then measure
    (p1 ph19):f2
    1m
    (p1 ph29):f2
    20u
    (p1 ph30):f2

    go=2 ph31
    1m wr #0 if #0

    ; increment WAHUHA time
    5m ip5*96
    5m ip14*96
    5m ip15*96
    5m ip16*96
    5m ip17*96
    5m ip18*96
    5m ip19*96
    5m iu1
    5m iu1
    lo to 2 times td1

  exit


