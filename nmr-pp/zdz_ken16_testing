; 12 October 2022
; Andrew Stasiuk
;
; DzZ Correlation under Ken16 interactions
; 11 deg Framechange
; theta=0

define delay cycle
"cycle=24*d3"

; td2 = n_max
; td1 = 10

define list<delay> tau1 = {0.00000449 0.00000499 0.00000549 0.00000599 0.00000649 0.00000699 0.00000749}
define list<delay> tau2 = {0.00000398 0.00000373 0.00000348 0.00000323 0.00000298 0.00000273 0.00000248}
define list<delay> tau3 = {0.00000898 0.00000848 0.00000798 0.00000748 0.00000698 0.00000648 0.00000598}
define list<delay> tau1p = {0.00000449 0.00000499 0.00000549 0.00000599 0.00000649 0.00000699 0.00000749}

"l1=0"

1  ze

; reset delay
2   d1 rpp0 rpp1 rpp2 rpp3 rpp4 rpp5 rpp6 rpp7
    cycle pl2:f2

    (p1 ph28):f2
    d2
    (p2 ph29):f2
    1m

; Kicking + Ken16
3
    tau1
    (p1 ph0^):f2
    tau2
    (p1 ph1^):f2
    tau3
    (p1 ph2^):f2
    tau2
    (p1 ph3^):f2
    tau1p

    tau1p
    (p1 ph4^):f2
    tau2
    (p1 ph5^):f2
    tau3
    (p1 ph6^):f2
    tau2
    (p1 ph7^):f2
    tau1

    lo to 3 times l1

    ; Measure
    1m
4   (p1 ph30):f2

    go=2 ph31

    1m wr #0 if #0

    ; increase evolution time by 1 cycle length, and re-scan
    1m iu1
    1m iu1
    lo to 2 times td2

    5m ru1
    5m tau1.inc
    5m tau2.inc
    5m tau3.inc
    5m tau1p.inc
    lo to 2 times td1

    exit


ph28 = (360) {349}*2 {169}*2
ph29 = {1}*2 {3}*2

