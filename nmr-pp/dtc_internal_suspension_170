; 21 July 2022
; Andrew Stasiuk
;
; ZZ Correlation under periodic kicking w/out transport
; Discrete Time Crystal Experiments
; no Framechange
; theta=175

; WAHUHA8 delays
define delay delay0
"delay0=(2*d20-p1)/2"
define delay delay1
"delay1=d20-p1"
define delay delay2
"delay2=(2*d20-p1)"

"l1=0"

1  ze

2   ; state & pp reset delay
    d1 rpp0 rpp1 rpp2 rpp3 rpp4 rpp5 rpp6 rpp7 rpp8 rpp9
    100u pl2:f2

; Hamiltonian Evolution
3   ; periodic kicking & scrambling

4   delay0
    (p1 ph0^):f2
    delay1
    (p1 ph1^):f2
    delay2
    (p1 ph2^):f2
    delay1
    (p1 ph3^):f2
    delay0

    delay0
    (p1 ph4^):f2
    delay1
    (p1 ph5^):f2
    delay2
    (p1 ph6^):f2
    delay1
    (p1 ph7^):f2
    delay0


    (p1 ph8^):f2
    2.5u ; Rz(theta)
    (p1 ph9^):f2

    lo to 3 times l1

    ; Measure
    d13
5   (p1 ph10):f2

    go=2 ph31

    1m wr #0 if #0

    ; increase evolution time by 1 cycle length, and re-scan
    5m iu1
    lo to 2 times td1

exit


; WAHUHA8
