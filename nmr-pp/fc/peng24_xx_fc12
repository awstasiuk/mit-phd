; with Framechange


#include <Avance.incl>

define delay tau0
"tau0=(d20-p1)/2"
define delay tau1
"tau1=(d20-p1)"

define delay cycle
"cycle=24*d20"

"l10=0"

1  ze

2  d1 rpp0 rpp1 rpp2 rpp3 rpp4 rpp5 rpp6 rpp7 rpp8 rpp9 rpp10 rpp11
   cycle pl2:f2
   (p1 ph28):f2

3
    tau0
    (p1 ph0^):f2
    tau1
    (p1 ph1^):f2
    tau1
    (p1 ph2^):f2
    tau1
    (p1 ph3^):f2
    tau1
    (p1 ph4^):f2
    tau1
    (p1 ph5^):f2
    tau1
    (p1 ph6^):f2
    tau1
    (p1 ph7^):f2
    tau1
    (p1 ph8^):f2
    tau1
    (p1 ph9^):f2
    tau1
    (p1 ph10^):f2
    tau1
    (p1 ph11^):f2
    tau0

  lo to 3 times l10

    2.5u
    (p1 ph29):f2
    1m
    (p1 ph30):f2

    go=2 ph31

    1m wr #0 if #0

    5m iu10
    5m iu10

    5m ip29*288

  lo to 2 times td1

exit


