; 18 January 2023
; Andrew Stasiuk
; ZZ Loschmidt echo under (cnst1)*XX - (cnst1)*YY
; No frame change

; cnst1 = {0, .02, .04, .06, .08, .10} (td1=6)
define list<delay> tau1 = {0.00000449 0.00000449 0.00000449 0.00000449 0.00000449 0.00000449}
define list<delay> tau2 = {0.00000398 0.00000388 0.00000378 0.00000368 0.00000358 0.00000348}
define list<delay> tau3 = {0.00000898 0.00000918 0.00000938 0.00000958 0.00000978 0.00000998}

define list<delay> tau1r = {0.00000449 0.00000449 0.00000449 0.00000449 0.00000449 0.00000449}
define list<delay> tau2r = {0.00000398 0.00000408 0.00000418 0.00000428 0.00000438 0.00000448}
define list<delay> tau3r = {0.00000898 0.00000878 0.00000858 0.00000838 0.00000818 0.00000798}

define delay cycle
"cycle=24*d3"

"l1=0"

1   ze
    ; increment the time reversal phase programs by fc*16
    25m ip4*176
    25m ip5*176
    25m ip6*176
    25m ip7*176

2
    d1 rpp0 rpp1 rpp2 rpp3 rpp4 rpp5 rpp6 rpp7
    cycle pl2:f2

3
    tau1
    (p1 ph0^):f2
    tau2
    (p1 ph1^):f2
    tau3
    (p1 ph2^):f2
    tau2
    (p1 ph3^):f2
    tau1

    lo to 3 times l1

4
    tau1r
    (p1 ph4^):f2
    tau2r
    (p1 ph5^):f2
    tau3r
    (p1 ph6^):f2
    tau2r
    (p1 ph7^):f2
    tau1r

    lo to 4 times l1

    1m
    (p1 ph30):f2

    go=2 ph31
    1m wr #0 if #0

    ; increase time evolution counters
    1m iu1
    1m iu1
    1m iu1
    1m iu1

    ; increment the time reversal phase programs by fc*16 to account for additional evolution
    25m ip4*176
    25m ip5*176
    25m ip6*176
    25m ip7*176
    lo to 2 times td2

    ; increment scrambling constants, reset phase programs
    5m tau1.inc
    5m tau2.inc
    5m tau3.inc

    5m tau1r.inc
    5m tau2r.inc
    5m tau3r.inc


    25m rp0
    25m rp1
    25m rp2
    25m rp3
    25m rp4
    25m rp5
    25m rp6
    25m rp7

    5m ru1
    lo to 2 times td1

  exit

ph0 = (360) 0 44 268 312 176 220 84 128 352 36 260 304 168 212 76 120 344 28 252 296 160 204 68 112 336 20 244 288 152 196 60 104 328 12 236 280 144 188 52 96 320 4 228 272 136 180 44 88 312 356 220 264 128 172 36 80 304 348 212 256 120 164 28 72 296 340 204 248 112 156 20 64 288 332 196 240 104 148 12 56 280 324 188 232 96 140 4 48 272 316 180 224 88 132 356 40 264 308 172 216
