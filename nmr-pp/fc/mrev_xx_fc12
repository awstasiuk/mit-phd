; d20 should be at least p1+2.5u

; WAHUHA8 delays
define delay delay0
"delay0=(2*d20-p1)/2"
define delay delay1
"delay1=d20-p1"
define delay delay2
"delay2=(2*d20-p1)"

define delay cycle
"cycle=12*d20";

"l10=0"

1 ze

2
  d1 rpp1 rpp2 rpp3 rpp0
  100u pl2:f2
  (p1 ph28):f2


