    (p1 ph0^):f2
    delay1
    (p1 ph1^):f2
    delay2
    (p1 ph2^):f2
    delay1
    (p1 ph3^):f2
    delay0
    lo to 3 times l10

    2.5u
    (p1 ph29):f2
    1m
    (p1 ph30):f2

   go=2 ph31


   50m wr #0 if #0

   5m iu10
   5m iu10
   
   5m ip29*96


   lo to 2 times td1


   exit

